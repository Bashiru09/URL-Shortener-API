generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")

}


model Url {
  id           Int      @id @default(autoincrement())
  originalUrl  String
  shortCode    String   @unique
  clicks       Int      @default(0)
  expiresAt    DateTime?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
